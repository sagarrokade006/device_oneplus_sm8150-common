        <version>2.3</version>
        <interface>
            <name>IBiometricsFingerprint</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="aidl">
        <name>android.hardware.bluetooth.audio</name>
        <version>4</version>
        <interface>
            <name>IBluetoothAudioProviderFactory</name>
